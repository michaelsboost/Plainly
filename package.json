{
  "name": "plainly",
  "version": "0.0.2",
  "scripts": {
    "build:css": "postcss src/bundle.css -o dist/bundle.css",
    "build:js": "esbuild src/concat.js --bundle --minify --sourcemap --outfile=dist/script.js",
    "build:c": "rollup -c",
    "build": "npm run build:css && npm run build:c && npm run build:js",
    "serve": "http-server -c-1 -p 8081"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "cssnano": "^7.0.2",
    "http-server": "^14.1.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.4.4",
    "terser": "^5.10.0"
  },
  "dependencies": {
    "docx": "^9.5.0",
    "esbuild": "^0.25.5",
    "marked": "^15.0.12"
  }
}
